
{
"name": "Trader Joes Power BI Theme with Schema", 
"$schema": "https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/refs/heads/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.136.json",
"dataColors":["#C81E24","#2C2C2C", "#F1E99C", "#093971", "#5f5f5f"],
"bad":"#580C13", 
"neutral":"#D5A543",
"good":"#3f9c6d",

"textClasses": {
        "callout": {
            "fontSize": 45,
            "fontFace": "Constantia",
            "color": "#2C2C2C"
        },
        "title": {
            "fontSize": 14,
            "fontFace": "Constantia",
            "color": "#2C2C2C"
        },
        "header": {
            "fontSize": 12,
            "fontFace": "Constantia",
            "color": "#2C2C2C"
        },
        "label": {
            "fontSize": 10,
            "fontFace": "Segoe UI",
            "color": "#2C2C2C"
        }

},

"visualStyles":{
    "page":{
        "*":{
            "background":[{
                "color":{
                    "solid":{
                        "color":"#FFFFFF"}},
                "transparency":0}],
            "outspace":[{
                 "color":{
                     "solid":{
                         "color":"#FAFBF8"}}}]}},
    "textbox": {
        "*": {
            "*": [{
                "fontSize": 24,
                "fontFamily": "Constantia",
                "wordWrap": true,
                "fontColor": {"solid": {"color":"#2C2C2C"}},
                "background": { "solid": { "color": "#FAFBF8"}}}],
                
            "border":[
                    {"show": true},
                    {"color":{"solid": {"color":"#2C2C2C"}}}]    
                }},                    

    "*":{

        "*":{ 
            "background": [
                {"color":{"solid": {"color":"#FAFBF8"}}}],

            "dropShadow": [
                {"show": true,
                "color": {"solid": {"color": "#5f5f5f"}},
                "position":"Outer",
                "preset":"BottomRight"}],
            
            "border":[
                    {"show": true},
                    {"color":{"solid": {"color":"#2C2C2C"}}}]

            


}

}

}
}